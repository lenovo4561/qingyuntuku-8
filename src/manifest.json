{
  "package": "com.qingyunjstk.qyposition",
  "name": "青云图库",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.app"
    },
    {
      "name": "system.request"
    },
    {
      "name": "system.media"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Launch",
    "pages": {
      "pages/Launch": {
        "component": "index"
      },
      "pkg/pages/Demo": {
        "component": "index"
      },
      "pkg/pages/DemoDetail": {
        "component": "index"
      },
      "pkg/pages/List": {
        "component": "index"
      },
      "pkg/pages/Web": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "pkg",
      "resource": "pkg"
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Launch": {
        "titleBar": false
      },
      "pkg/pages/Demo": {
        "titleBar": false,
        "menu": false
      },
      "pkg/pages/DemoDetail": {
        "titleBar": false
      },
      "pkg/pages/List": {
        "titleBar": false,
        "titleBarText": "列表"
      },
      "pkg/pages/Web": {
        "titleBarText": "详情"
      }
    }
  },
  "deviceTypeList": ["phone"]
}
